      "source": [
        "\n",
        "import io\n",
        "import os\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "from PyPDF2 import PdfReader\n",
        "from docx import Document\n",
        "from sentence_transformers import SentenceTransformer\n",
        "from transformers import pipeline\n",
        "import faiss\n",
        "\n",
        "# ---------------- Document loaders ----------------\n",
        "\n",
        "def load_txt(file_bytes, name):\n",
        "    text = file_bytes.decode('utf-8', errors='ignore')\n",
        "    return [{\"source\": name, \"text\": text}]\n",
        "\n",
        "def load_pdf(file_bytes, name):\n",
        "    reader = PdfReader(io.BytesIO(file_bytes))\n",
        "    text = \"\"\n",
        "    for page in reader.pages:\n",
        "        text += page.extract_text() or \"\"\n",
        "    return [{\"source\": name, \"text\": text}]\n",
        "\n",
        "def load_docx(file_bytes, name):\n",
        "    doc = Document(io.BytesIO(file_bytes))\n",
        "    text = \" \".join([para.text for para in doc.paragraphs])\n",
        "    return [{\"source\": name, \"text\": text}]\n",
        "\n",
        "def load_csv(file_bytes, name):\n",
        "    df = pd.read_csv(io.BytesIO(file_bytes))\n",
        "    text = df.astype(str).values.flatten()\n",
        "    return [{\"source\": name, \"text\": \" \".join(text)}]\n",
        "\n",
        "def load_documents_from_paths(paths):\n",
        "    \"\"\"\n",
        "    paths: list of local file paths (uploaded to Colab VM) ending with .pdf/.docx/.csv/.txt\n",
        "    \"\"\"\n",
        "    docs = []\n",
        "    for p in paths:\n",
        "        with open(p, 'rb') as f:\n",
        "            data = f.read()\n",
        "        if p.lower().endswith('.pdf'):\n",
        "            docs += load_pdf(data, os.path.basename(p))\n",
        "        elif p.lower().endswith('.docx'):\n",
        "            docs += load_docx(data, os.path.basename(p))\n",
        "        elif p.lower().endswith('.csv'):\n",
        "            docs += load_csv(data, os.path.basename(p))\n",
        "        elif p.lower().endswith('.txt'):\n",
        "            docs += load_txt(data, os.path.basename(p))\n",
        "    return docs\n",
        "\n",
        "# ---------------- Webpage loader with User-Agent ----------------\n",
        "\n",
        "def fetch_web_text(url):\n",
        "    try:\n",
        "        headers = {'User-Agent': 'Mozilla/5.0'}\n",
        "        resp = requests.get(url, headers=headers, timeout=15)\n",
        "        resp.raise_for_status()\n",
        "        soup = BeautifulSoup(resp.text, 'html.parser')\n",
        "        for s in soup([\"script\", \"style\", \"noscript\"]):\n",
        "            s.decompose()\n",
        "        text = soup.get_text(separator=' ')\n",
        "        return [{\"source\": url, \"text\": text}]\n",
        "    except Exception as e:\n",
        "        print(f\"Failed to fetch {url}: {e}\")\n",
        "        return []\n",
        "\n",
        "def load_documents_from_urls(urls):\n",
        "    docs = []\n",
        "    for u in urls:\n",
        "        u = u.strip()\n",
        "        if not u:\n",
        "            continue\n",
        "        docs += fetch_web_text(u)\n",
        "    return docs\n",
        "\n",
        "# ---------------- Text chunking ----------------\n",
        "\n",
        "def chunk_documents(docs, chunk_size=1000, chunk_overlap=100):\n",
        "    # Simple character-based splitter to avoid extra dependencies\n",
        "    chunks = []\n",
        "    for doc in docs:\n",
        "        text = doc[\"text\"] or \"\"\n",
        "        start = 0\n",
        "        idx = 0\n",
        "        while start < len(text):\n",
        "            end = start + chunk_size\n",
        "            chunk = text[start:end]\n",
        "            chunks.append({\n",
        "                \"source\": doc[\"source\"],\n",
        "                \"chunk_id\": f\"{doc['source']}_chunk{idx}\",\n",
        "                \"content\": chunk\n",
        "            })\n",
        "            idx += 1\n",
        "            start = end - chunk_overlap\n",
        "            if start < 0:\n",
        "                start = 0\n",
        "    return chunks\n",
        "\n",
        "# ---------------- Embeddings ----------------\n",
        "\n",
        "_embedder = None\n",
        "def load_embedder():\n",
        "    global _embedder\n",
        "    if _embedder is None:\n",
        "        # 384-d embeddings\n",
        "        _embedder = SentenceTransformer('sentence-transformers/all-MiniLM-L6-v2')\n",
        "    return _embedder\n",
        "\n",
        "def get_embeddings(chunks, embedder):\n",
        "    texts = [c['content'] for c in chunks]\n",
        "    emb = embedder.encode(texts, show_progress_bar=True, convert_to_numpy=True, normalize_embeddings=False)\n",
        "    emb = emb.astype('float32')\n",
        "    return emb\n",
        "\n",
        "# ---------------- FAISS index ----------------\n",
        "\n",
        "def build_faiss_index(embeddings):\n",
        "    dim = embeddings.shape[1]\n",
        "    index = faiss.IndexFlatL2(dim)  # exact L2 search\n",
        "    index.add(embeddings)\n",
        "    return index\n",
        "\n",
        "# ---------------- Retriever ----------------\n",
        "\n",
        "def retrieve(query, embedder, index, chunks, top_k=3):\n",
        "    q_emb = embedder.encode([query], convert_to_numpy=True).astype('float32')\n",
        "    distances, indices = index.search(q_emb, top_k)\n",
        "    out = []\n",
        "    for rank, i in enumerate(indices):\n",
        "        if i == -1:\n",
        "            continue\n",
        "        out.append({\"chunk\": chunks[i], \"score\": float(distances[rank])})\n",
        "    return out\n",
        "\n",
        "# ---------------- LLM ----------------\n",
        "\n",
        "_llm = None\n",
        "def load_llm():\n",
        "    global _llm\n",
        "    if _llm is None:\n",
        "        # Small, open model for local generation; change if needed\n",
        "        _llm = pipeline(\n",
        "            \"text-generation\",\n",
        "            model=\"MehdiHosseiniMoghadam/AVA-Mistral-7B-V2\",\n",
        "            tokenizer=\"MehdiHosseiniMoghadam/AVA-Mistral-7B-V2\",\n",
        "            max_length=1024,\n",
        "            do_sample=True,\n",
        "            temperature=0.2,\n",
        "            trust_remote_code=True,\n",
        "            device_map=\"auto\"\n",
        "        )\n",
        "    return _llm\n",
        "\n",
        "def answer_with_llm(context_chunks, query, llm):\n",
        "    context_text = \"\\n\".join([f\"[{c['chunk_id']}] {c['content']}\" for c in context_chunks])\n",
        "    prompt = (\n",
        "        \"Answer the following question using ONLY the provided context and cite the chunk ids used.\\n\"\n",
        "        f\"Question: {query}\\n\"\n",
        "        \"Context:\\n\"\n",
        "        f\"{context_text}\\n\"\n",
        "        \"Answer with citations:\"\n",
        "    )\n",
        "    out = llm(prompt, max_new_tokens=512, num_return_sequences=1)\n",
        "    if isinstance(out, list) and len(out) > 0 and 'generated_text' in out:\n",
        "        return out['generated_text']\n",
        "    # For some versions, pipeline returns dict\n",
        "    return str(out)\n",
        "\n",
        "# ---------------- Example main ----------------\n",
        "\n",
        "def run_rag(\n",
        "    file_paths=None,\n",
        "    urls=None,\n",
        "    query=\"What are the key points?\",\n",
        "    chunk_size=1000,\n",
        "    chunk_overlap=100,\n",
        "    top_k=3\n",
        "):\n",
        "    file_paths = file_paths or []\n",
        "    urls = urls or []\n",
        "\n",
        "    file_docs = load_documents_from_paths(file_paths)\n",
        "    web_docs = load_documents_from_urls(urls)\n",
        "    all_docs = file_docs + web_docs\n",
        "\n",
        "    if not all_docs:\n",
        "        print(\"No documents loaded. Provide file paths or URLs.\")\n",
        "        return None\n",
        "\n",
        "    print(f\"Loaded {len(all_docs)} document(s).\")\n",
        "    chunks = chunk_documents(all_docs, chunk_size=chunk_size, chunk_overlap=chunk_overlap)\n",
        "    print(f\"Created {len(chunks)} chunks.\")\n",
        "\n",
        "    embedder = load_embedder()\n",
        "    embeddings = get_embeddings(chunks, embedder)\n",
        "    index = build_faiss_index(embeddings)\n",
        "\n",
        "    llm = load_llm()\n",
        "\n",
        "    retrieved = retrieve(query, embedder, index, chunks, top_k=top_k)\n",
        "    ctx_chunks = [r[\"chunk\"] for r in retrieved]\n",
        "    answer = answer_with_llm(ctx_chunks, query, llm)\n",
        "\n",
        "    print(\"\\n=== Answer ===\\n\")\n",
        "    print(answer)\n",
        "\n",
        "    print(\"\\n=== Sources (chunk ids) ===\\n\")\n",
        "    for r in retrieved:\n",
        "        print(f\"[{r['chunk']['chunk_id']}] from {r['chunk']['source']} (score={r['score']:.4f})\")\n",
        "\n",
        "    return {\n",
        "        \"answer\": answer,\n",
        "        \"retrieved\": retrieved\n",
        "    }\n",
        "\n",
        "# ---------------- How to use in Colab ----------------\n",
        "# 1) Upload files: from the left file browser or via:\n",
        "#   from google.colab import files\n",
        "#   uploaded = files.upload()  # then file_paths = list(uploaded.keys())\n",
        "#\n",
        "# 2) Or just pass URLs.\n",
        "#\n",
        "# Example:\n",
        "# result = run_rag(\n",
        "#     file_paths=[\"/content/sample.txt\"],  # adjust if uploaded\n",
        "#     urls=[\"https://huggingface.co/sentence-transformers/all-MiniLM-L6-v2\"],\n",
        "#     query=\"What is the embedding dimension and how to use this model?\",\n",
        "#     top_k=3\n",
        "# )"
      ]
    },
